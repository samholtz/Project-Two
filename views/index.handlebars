<div id="page-content-wrapper">
  <div class="container-fluid col-10 mx-auto">

    <a href="#menu-toggle" class="btn btn-outline-dark btn-sm" id="menu-toggle">Menu</a><br><br>
    <div class="container">
      <h1>Welcome to the Jurassic Perk!</h1><br>
      <p>Your dinosaurs will appear below once you purchase the disosaur of your choice. Visit the Petting Zoo to get
        started! Need some money? Visit the Money Farm or Play a game to collect Dino Bucks. Good Luck!</p><br>

      <div class="card-deck">
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            {{#each result}}
            <div id="space" class="d-inline-flex p-2" "mr-2">
              <div class="card" style="width: 18rem;">
                <div class="card-body" "d-inline-flex p-2">
                  <ul>
                    <img class="card-img-top" src={{img}} alt="Card image cap">
                    <h5 class="card-text">Dino Name: {{name}} </h5>
                    <p class="card-text">Speed: {{speed}}</p>
                    <p class="card-text">Power: {{power}}</p>
                    <a href="#" class="btn btn-primary">Click to Use</a>
                  </ul>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- /#wrapper -->

<!-- Bootstrap core JavaScript -->
<script src='https://code.jquery.com/jquery-3.3.1.min.js' integrity='sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8='
  crossorigin='anonymous'></script>

<!-- Menu Toggle Script -->
<script>
  $("#menu-toggle").click(function (e) {
    e.preventDefault();
    $("#wrapper").toggleClass("toggled");
  });
</script>